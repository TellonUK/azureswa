{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "auth": {
    "identityProviders": {
      "openIdConnectProviders": {
        "azureb2c": {
          "registration": {
            "clientIdSettingName": "<SWACLIENTID>",
            "clientCredential": {
              "secretSettingName": "<SWASECRET>"
            },
            "openIdConnectConfiguration": {
              "wellKnownOpenIdConfiguration": "<SWAWELLKNOWN>"
            }
          }
        }
      }
    }
  }
}
